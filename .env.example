# Example .env for Odoo MCP Server

# Odoo API Key (get from your Odoo user profile)
ODOO_API_KEY=

# Odoo instance URL (e.g. https://my-odoo.example.com)
ODOO_URL=

# Odoo database name (e.g. mydb)
ODOO_DB=

# Odoo Extract API account token (for document parsing)
EXTRACT_ACCOUNT_TOKEN=

# Copy this file to .env and fill in your actual credentials.
# Example:
#   cp .env.example .env
